[Unit]
Description=SentinelZero Loopback Sentinels Setup
After=network.target
Before=sentinelzero-backend.service
PartOf=sentinelzero-backend.service

[Service]
Type=oneshot
User=root
Group=root
ExecStart=/home/vscode/sentinelZero/setup-loopbacks.sh
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
