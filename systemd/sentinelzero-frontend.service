[Unit]
Description=SentinelZero Frontend
After=network.target

[Service]
Type=simple
User=sentinelzero
Group=sentinelzero
WorkingDirectory=/home/sentinel/SentinelZero/frontend/react-sentinelzero
ExecStart=/usr/bin/npm run dev -- --host 0.0.0.0 --port 5173
Restart=on-failure

[Install]
WantedBy=multi-user.target
